<node>
  <interface name="com.canonical.applications.Downloader">
    <method name="createDownload">
        <arg name="url" type="s" direction="in"/>
        <arg name="download" type="o" direction="out" />
    </method>

    <method name="createDownloadWithHash">
        <arg name="url" type="s" direction="in"/>
        <arg name="algorithm" type="s" direction="in"/>
        <arg name="hash" type="s" direction="in"/>
        <arg name="download" type="o" direction="out" />
    </method>

    <method name="getAllDownloads">
        <arg name="downloads" type="ao" direction="out" />
    </method>

    <signal name="downloadCreated">
        <arg name="path" type="o" direction="out"/>
    </signal>
 </interface>
</node>
